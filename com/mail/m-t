.key link/s,tidy/s,ancilla/s,opt/k,ws/k,cc/k,reflect/s,sl/s,sc/s,all/s,comp/k,b1/k,b2/k,la/s
.def    b1      g1:bcp.
.def    b2      bcp.
.def    comp    bcp
.def    dest    t:
.def    opt     -wa
.def    reflect c

IF MCTYPE=LSI4; <reflect> t:make-c lsi4  ws=<ws$25000> opt=-db<opt>/SN s2=/sl/ Md=". " <link> <tidy> <ancilla> <sl> <sc> <all> <la>
IF MCTYPE=68000;<reflect> t:make-c 68000 ws=<ws$35000> opt= lo<opt>/SN         Ld=". " <link> <tidy> <ancilla> <sl> <sc> <all> <la>


