$<ERRHDR'
$$ERRHDR := TRUE

$<COMHDR'       GET "COMHDR"    $>COMHDR'

GLOBAL
$(      SETTING         : ERR+  0       // Local global-variable

        NEXTSW          : ERR+  1       // Local global routines
//      TASKWRITEF      : ERR+  2
        Fatal.error     : ERR+  3
        CGXINIT         : ERR+  4
        CGXREPORT       : ERR+  5
        CGPARMS         : ERR+  6
        CGMESSAGE       : ERR+  7
        CGXSWITCH       : ERR+  8
$)
$>ERRHDR'


